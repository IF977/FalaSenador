<p id="notice"><%= notice %></p>

<h1>Senators</h1>

<table>
  <thead>
    <tr>
      <th>Codigoparlamentar</th>
      <th>Nome</th>
      <th>Urlfoto</th>
      <th>Urlpagina</th>
      <th>Siglapartido</th>
      <th>Uf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @senators.each do |senator| %>
      <tr>
        <td><%= senator.codigoparlamentar %></td>
        <td><%= senator.nome %></td>
        <td><%= senator.urlfoto %></td>
        <td><%= senator.urlpagina %></td>
        <td><%= senator.siglapartido %></td>
        <td><%= senator.uf %></td>
        <td><%= link_to 'Show', senator %></td>
        <td><%= link_to 'Edit', edit_senator_path(senator) %></td>
        <td><%= link_to 'Destroy', senator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Senator', new_senator_path %>
