<p id="notice"><%= notice %></p>

<h1>Speeches</h1>

<table>
  <thead>
    <tr>
      <th>Codigoparlamentar</th>
      <th>Codigopronunciamento</th>
      <th>Data</th>
      <th>Urltexto</th>
      <th>Textocompleto</th>
      <th>Senator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @speeches.each do |speech| %>
      <tr>
        <td><%= speech.CodigoParlamentar %></td>
        <td><%= speech.CodigoPronunciamento %></td>
        <td><%= speech.Data %></td>
        <td><%= speech.UrlTexto %></td>
        <td><%= speech.TextoCompleto %></td>
        <td><%= speech.senator %></td>
        <td><%= link_to 'Show', speech %></td>
        <td><%= link_to 'Edit', edit_speech_path(speech) %></td>
        <td><%= link_to 'Destroy', speech, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Speech', new_speech_path %>
